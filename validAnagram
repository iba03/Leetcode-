 bool isAnagram(char* s, char* t) {
    int arr[26]={0};
    int i;
    for(i=0;i<strlen(s);i++)
    {
        arr[s[i]-'a']++;
    }
    for(i=0;i<strlen(t);i++)
    {
        arr[t[i]-'a']--;
    }
    for(i=0;i<26;i++)
    {
        if(arr[i]!=0)
        {
            return false;

        }
    }
    return true;
    
}
